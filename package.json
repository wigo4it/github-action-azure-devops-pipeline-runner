{
  "name": "github-action-azure-devops-pipeline-runner",
  "version": "1.0.0",
  "description": "GitHub Action to run Azure DevOps pipelines using Managed Identity authentication",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build src/main.ts -o dist --license licenses.txt",
    "package:dev": "npm run package -- --source-map",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "all": "npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wigo4it/github-action-azure-devops-pipeline-runner.git"
  },
  "keywords": [
    "actions",
    "azure-devops",
    "pipeline",
    "managed-identity"
  ],
  "author": "wigo4it",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3"
  }
}
